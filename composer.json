{
    "name": "pdir/contao-alleaktien-quantitativ-bundle",
    "type": "contao-bundle",
    "description": "AlleAktien Quantitativ Contao bundle",
    "homepage": "https://alleaktien.de/quantitativ/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Finanznews, Aktiennews & Tools",
            "homepage": "https://finanzsecura.de"
        },
        {
            "name": "pdir GmbH",
            "homepage": "https://pdir.de"
        }

    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4 || ^5.1",
        "symfony/dependency-injection": "^4.4 || ^5.1",
        "symfony/http-kernel": "^4.4 || ^5.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^2.0",
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.1"
    },
    "extra": {
        "contao-manager-plugin": "Pdir\\AlleAktienQuantitativBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Pdir\\AlleAktienQuantitativBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdir\\AlleAktienQuantitativBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pdir/contao-alleaktien-quantitativ-bundle/issues",
        "source": "https://github.com/pdir/contao-alleaktien-quantitativ-bundle"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ tests/ --fix --ansi"
        ]
    }
}
